{
  "$schema": "vscode://schemas/color-theme",
  "name": "SPARC Dark",
  "type": "dark",
  "tokenColors": [

    {
      "scope": "comment.line.sparc",
      "settings": { "foreground": "#5ea13f", "fontStyle": "italic" }
    },
    {
      "scope": "keyword.directive.sparc",
      "settings": { "foreground": "#569CD6" }
    },
    {
      "scope": "variable.parameter.sparc",
      "settings": { "foreground": "#00BCD4" }
    },
    {
      "scope": "constant.numeric.sparc",
      "settings": { "foreground": "#B5CEA8" }
    },
    {
      "scope": "keyword.sorts.section.sparc",
      "settings": { "foreground": "#C586C0", "fontStyle": "bold" }
    },
    {
      "scope": "keyword.predicates.section.sparc",
      "settings": { "foreground": "#C586C0", "fontStyle": "bold" }
    },
    {
      "scope": "keyword.rules.section.sparc",
      "settings": { "foreground": "#C586C0", "fontStyle": "bold" }
    },
     {
      "scope": "keyword.sort.declaration.sparc",
      "settings": { "foreground": "#569CD6" }
    },
    {
      "scope": "keyword.objects.sparc",
      "settings": { "foreground": "#2fce9f" }
    },
    {
      "scope": "keyword.predicates.declaration.sparc",
      "settings": { "foreground": "#DCDCAA" }
    },
    {
      "scope": "variable.sparc",
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "scope": "keyword.operator.rule.sparc",
      "settings": { "foreground": "#D4D4D4" }
    },
    {
      "scope": "keyword.operator.cr-rule.sparc",
      "settings": { "foreground": "#D4D4D4" }
    },
    {
      "scope": "keyword.operator.not.sparc",
      "settings": { "foreground": "#D4D4D4" }
    },
    {
      "scope": "keyword.operator.comma.sparc",
      "settings": { "foreground": "#D4D4D4" }
    }
  ]
}
